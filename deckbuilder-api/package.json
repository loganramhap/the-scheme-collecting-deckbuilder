{
  "name": "deckbuilder-api",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "node --watch src/index.js",
    "start": "node src/index.js",
    "setup:dynamodb": "node src/db/setup.js",
    "db:migrate": "node src/db/migrate.js",
    "db:backup": "node src/db/backup.js create",
    "db:export": "node src/db/backup.js export",
    "db:list-backups": "node src/db/backup.js list",
    "test": "vitest --run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.478.0",
    "@aws-sdk/lib-dynamodb": "^3.478.0",
    "axios": "^1.6.2",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^8.2.1"
  },
  "devDependencies": {
    "@vitest/ui": "^4.0.8",
    "supertest": "^6.3.3",
    "vitest": "^4.0.8"
  }
}
